! font config

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcdfilter

! xterm

xterm.termName: xterm-256color
xterm*saveLines: 4096
xterm*locale: true
xterm*loginShell: true
xterm*internalBorder: 17
xterm*faceName: xft:Hack Nerd Font
xterm*faceSize: 11
xterm*renderFont: true
xterm*cursorBlink: false
xterm*ScrollBar: false
xterm.vt100.backarrowKey: false
xterm.ttyModes: erase ^?

xterm.vt100.translations: #override \n\
  Ctrl <Key> -: smaller-vt-font() \n\
  Ctrl <Key> =: larger-vt-font()

! rofi

rofi.color-enabled: true
rofi.color-window: #161821, #161821, #161821
rofi.color-normal: #161821, #c6c8d1, #161821, #424242, #c6c8d1
rofi.color-active: #161821, #c6c8d1, #161821, #424242, #c6c8d1
rofi.color-urgent: #161821, #c6c8d1, #161821, #424242, #c6c8d1
rofi.width: 30
rofi.lines: 5
rofi.columns: 2
rofi.font: Hack Nerd Font 14
rofi.bw: 0
rofi.padding: 5
rofi.eh: 1
rofi.separator-style: none
rofi.hide-scrollbar: true

! cursor theme

Xcursor.theme: PearWhiteCursors

! color scheme


! special
*.foreground:   #c6c8d1
*.background:   #161821
*.cursorColor:  #c6c8d1

! black
*.color0  : #161821
*.color8  : #22262e

! red
*.color1  : #e27878
*.color9  : #e98989

! green
*.color2  : #b4be82
*.color10 : #c0ca8e

! yellow
*.color3  : #e2a478
*.color11 : #e9b189

! blue
*.color4  : #84a0c6
*.color12 : #91acd1

! magenta
*.color5  : #a093c7
*.color13 : #ada0d3

! cyan
*.color6  : #89b8c2
*.color14 : #95c4ce

! white
*.color7  : #c6c8d1
*.color15 : #d2d4de